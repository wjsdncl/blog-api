{
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "@supabase/supabase-js": "^2.47.7",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "es-hangul": "^2.1.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "is-email": "^1.0.2",
    "is-uuid": "^1.0.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.3",
    "sharp": "^0.33.5",
    "superstruct": "^1.0.4",
    "winston": "^3.17.0",
    "zod": "^3.22.4",
    "zod-prisma-types": "^3.1.8"
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "prisma": "^6.11.1",
    "typescript": "^5.3.3",
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/sharp": "^0.32.0",
    "tsx": "^4.6.2",
    "@typescript-eslint/parser": "^6.15.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0"
  },
  "type": "module",
  "scripts": {
    "dev": "tsx watch app.ts",
    "start": "node dist/app.js",
    "build": "tsc",
    "build:full": "npm ci && npx prisma generate && tsc",
    "build:with-db": "npm ci && npx prisma generate && npx prisma migrate deploy && tsc",
    "build:with-data": "npm ci && npx prisma generate && npx prisma migrate deploy && npm run migrate:blog-data && tsc",
    "build:render": "npx prisma generate && npx prisma migrate deploy && npm run migrate:blog-data && tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "dev:js": "nodemon app.js",
    "start:js": "node app.js",
    "db:reset:safe": "node scripts/safe-reset.js",
    "db:backup": "echo 'Please create manual backup before any migration'",
    "format:prisma": "prisma format",
    "migrate:blog-data": "node prisma/migrate-blog-data.js",
    "deploy:migrate": "prisma migrate deploy && npm run migrate:blog-data"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
